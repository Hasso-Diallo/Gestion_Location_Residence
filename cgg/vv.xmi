<?xml version="1.0" encoding="UTF-8"?>
<uml:Model xmi:version="2.1" xmlns:xmi="http://www.omg.org/XMI" xmlns:uml="http://www.eclipse.org/uml2/3.0.0/UML">
    <packagedElement xmi:type="uml:Class" xmi:id="Gestionnaire" name="Gestionnaire">
        <ownedAttribute xmi:type="uml:Property" name="nom" type="String"/>
        <ownedAttribute xmi:type="uml:Property" name="email" type="String"/>
    </packagedElement>
    
    <packagedElement xmi:type="uml:Class" xmi:id="Locataire" name="Locataire">
        <ownedAttribute xmi:type="uml:Property" name="nom" type="String"/>
        <ownedAttribute xmi:type="uml:Property" name="email" type="String"/>
    </packagedElement>
    
    <packagedElement xmi:type="uml:Class" xmi:id="ÉlémentRésidence" name="ÉlémentRésidence">
        <ownedAttribute xmi:type="uml:Property" name="nom" type="String"/>
        <ownedAttribute xmi:type="uml:Property" name="tarifs" type="List<Tarif>"/>
        <ownedAttribute xmi:type="uml:Property" name="periodesIndisponibles" type="List<Période>"/>
    </packagedElement>
    
    <packagedElement xmi:type="uml:Class" xmi:id="Tarif" name="Tarif">
        <ownedAttribute xmi:type="uml:Property" name="periode" type="String"/>
        <ownedAttribute xmi:type="uml:Property" name="prix" type="Double"/>
    </packagedElement>
    
    <packagedElement xmi:type="uml:Class" xmi:id="Période" name="Période">
        <ownedAttribute xmi:type="uml:Property" name="debut" type="Date"/>
        <ownedAttribute xmi:type="uml:Property" name="fin" type="Date"/>
    </packagedElement>
    
    <packagedElement xmi:type="uml:Class" xmi:id="Réservation" name="Réservation">
        <ownedAttribute xmi:type="uml:Property" name="dateDebut" type="Date"/>
        <ownedAttribute xmi:type="uml:Property" name="dateFin" type="Date"/>
    </packagedElement>
    
    <packagedElement xmi:type="uml:Association" xmi:id="Gestionnaire_Residence" memberEnd="Gestionnaire ÉlémentRésidence"/>
    <packagedElement xmi:type="uml:Association" xmi:id="Locataire_Reservation" memberEnd="Locataire Réservation"/>
    <packagedElement xmi:type="uml:Association" xmi:id="Reservation_Element" memberEnd="Réservation ÉlémentRésidence"/>
</uml:Model>
